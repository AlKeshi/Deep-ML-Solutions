   "source": [
    "import numpy as np\n",
    "\n",
    "def calculate_eigenvalues(matrix: list[list[float|int]]) -> list[float]:\n",
    "\tL = np.linalg.eigvals(np.array(matrix)).tolist()\n",
    "\tL.sort(reverse=True)\n",
    "\treturn L"
   ]
  }
